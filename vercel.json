{
  "version": 2,
  "builds": [
    {
      "src": "api/agent.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/agent.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "api/agent.py": {
      "maxDuration": 30
    }
  }
}
